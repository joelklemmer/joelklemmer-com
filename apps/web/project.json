{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "tags": ["scope:web", "type:app"],
  "// targets": "to see all targets run: nx show project web --web",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/web"
      }
    },
    "bundle-guard": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-route-chunks.ts"
      }
    },
    "server-import-graph-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-server-import-graph.ts"
      }
    },
    "critical-bundle-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-critical-bundle-contains-no-heavy-modules.ts"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx next start",
        "cwd": "{projectRoot}"
      },
      "configurations": {
        "production": {}
      }
    },
    "dev:stable": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx next dev --no-turbopack",
        "cwd": "{projectRoot}"
      },
      "description": "Dev server without Turbopack; use if 'IO error attempting to create and acquire lockfile (os error 5)' on Windows. If issues persist, remove .next and retry."
    },
    "a11y": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/run-a11y.ts"
      }
    },
    "a11y-brief-repro": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/run-a11y-brief-only.ts"
      },
      "description": "Diagnostics: 10 iterations per locale on /brief to reproduce intermittent a11y failures; not in verify"
    },
    "visual": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/run-visual.ts"
      }
    },
    "lighthouse": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/run-lighthouse.ts"
      }
    },
    "lighthouse-timespan": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/run-lighthouse-timespan.ts"
      }
    },
    "content-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-content.ts"
      }
    },
    "docs-integrity-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-docs-integrity.ts"
      }
    },
    "governance-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-governance.ts"
      }
    },
    "compliance-registry-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-compliance-registry.ts"
      }
    },
    "tracker-usage-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-tracker-usage.ts"
      }
    },
    "telemetry-governance-validate": {
      "executor": "nx:run-commands",
