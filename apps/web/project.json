        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-cookie-compliance.ts"
      }
    },
    "security-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-security-txt.ts"
      }
    },
    "lighthouse-budget": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/lighthouse-budget-stub.ts"
      }
    },
    "perf-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-perf.ts"
      }
    }
