      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node -e \"console.log('No web tests configured')\""
      }
    },
    "restore-generated-typings": {
      "executor": "nx:run-commands",
      "options": {
        "command": "git checkout -- apps/web/next-env.d.ts"
      }
    },
    "validate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx format:check --all",
          "nx run web:lint",
          "nx run web:content-validate",
          "nx run web:cookie-compliance-validate",
          "nx run web:docs-integrity-validate",
          "nx run web:governance-validate",
          "nx run web:compliance-registry-validate",
          "nx run web:tracker-usage-validate",
          "nx run web:telemetry-governance-validate",
          "nx run web:i18n-validate",
          "nx run web:pgf-validate",
          "nx run web:intelligence-validate",
          "nx run web:briefing-contracts-validate",
          "nx run web:authority-signals-validate",
          "nx run web:experience-intelligence-validate",
          "nx run web:frameworks-validate",
          "nx run web:aec-validate",
          "nx run web:orchestration-validate",
          "nx run web:content-os-validate",
          "nx run web:home-validate",
          "nx run web:sitemap-validate",
          "nx run web:image-sitemap-validate",
          "nx run web:media-manifest-validate",
          "nx run web:media-derivatives-validate",
          "nx run web:media-authority-validate",
          "nx run web:media-governance-validate",
          "nx run web:visual-contract-validate",
          "nx run web:lighthouse-config-validate",
          "nx run web:seo-validate",
          "nx run web:tokens-validate",
          "nx run web:token-drift-validate",
          "nx run web:figma-source-validate",
          "nx run web:authority-program-validate",
          "nx run web:authority-constitution-validate",
          "nx run web:agent-overlay-validate",
          "nx run web:telemetry-scoring-validate",
          "nx run docs:arch:check",
          "nx run web:test"
        ],
        "parallel": false
      }
    },
    "verify": {
      "executor": "nx:run-commands",
      "options": {
        "env": {
          "NEXT_PUBLIC_SITE_URL": "https://example.invalid",
          "NEXT_PUBLIC_IDENTITY_SAME_AS": "https://example.invalid/ci",
          "STANDALONE": "1",
          "RATE_LIMIT_MODE": "off"
        },
        "commands": [
          "node -e \"try{require('fs').rmSync(require('path').join(process.cwd(),'apps/web/.next'),{recursive:true,force:true});}catch(e){}\"",
          "nx format:check --all",
          "pnpm audit --audit-level=critical",
          "nx run web:lint",
          "nx run web:security-validate",
          "nx run web:cookie-compliance-validate",
          "nx run web:content-validate",
          "nx run web:docs-integrity-validate",
          "nx run web:governance-validate",
          "nx run web:compliance-registry-validate",
          "nx run web:tracker-usage-validate",
          "nx run web:telemetry-governance-validate",
          "nx run web:i18n-validate",
          "nx run web:pgf-validate",
          "nx run web:intelligence-validate",
          "nx run web:briefing-contracts-validate",
          "nx run web:authority-signals-validate",
          "nx run web:experience-intelligence-validate",
          "nx run web:frameworks-validate",
          "nx run web:aec-validate",
          "nx run web:orchestration-validate",
          "nx run web:content-os-validate",
          "nx run web:home-validate",
          "nx run web:sitemap-validate",
          "nx run web:image-sitemap-validate",
          "nx run web:media-manifest-validate",
          "nx run web:media-derivatives-validate",
          "nx run web:media-authority-validate",
          "nx run web:media-governance-validate",
          "nx run web:visual-contract-validate",
          "nx run web:lighthouse-config-validate",
          "nx run web:seo-validate",
          "nx run web:tokens-validate",
          "nx run web:token-drift-validate",
          "nx run web:figma-source-validate",
          "nx run web:authority-program-validate",
          "nx run web:authority-constitution-validate",
          "nx run web:agent-overlay-validate",
          "nx run web:telemetry-scoring-validate",
          "nx run docs:arch:check",
          "nx run web:test",
          "nx run web:build",
          "nx run web:bundle-guard",
          "nx run web:server-import-graph-validate",
          "nx run web:critical-bundle-validate",
          "nx run web:restore-generated-typings",
          "nx run web:lighthouse-harness-validate",
          "nx run web:head-invariants-validate",
          "npx cross-env SKIP_A11Y_BUILD=1 nx run web:a11y",
          "npx cross-env SKIP_RATE_LIMIT_BUILD=1 nx run web:rate-limit-verify"
        ],
        "parallel": false
      }
    },
