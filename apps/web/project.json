{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "tags": ["scope:web", "type:app"],
  "// targets": "to see all targets run: nx show project web --web",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/web"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx next start",
        "cwd": "{projectRoot}"
      },
      "configurations": {
        "production": {}
      }
    },
    "a11y": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/run-a11y.ts"
      }
    },
    "content-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-content.ts"
      }
    },
    "docs-integrity-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-docs-integrity.ts"
      }
    },
    "governance-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-governance.ts"
      }
    },
    "compliance-registry-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-compliance-registry.ts"
      }
    },
    "tracker-usage-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-tracker-usage.ts"
      }
    },
    "telemetry-governance-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-telemetry-governance.ts"
      }
    },
    "i18n-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-i18n.ts"
      }
    },
    "pgf-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-pgf.ts"
      }
    },
    "intelligence-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-intelligence.ts"
      }
    },
    "briefing-contracts-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-briefing-contracts.ts"
      }
    },
    "authority-signals-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-authority-signals.ts"
      }
    },
    "telemetry-scoring-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-telemetry-scoring.ts"
      }
    },
    "experience-intelligence-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-experience-intelligence.ts"
      }
    },
    "frameworks-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-frameworks.ts"
      }
    },
    "aec-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-aec.ts"
      }
    },
    "orchestration-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-orchestration.ts"
      }
    },
    "content-os-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-content-os.ts"
      }
    },
    "home-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-home.ts"
      }
    },
    "sitemap-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-sitemap.ts"
      }
    },
    "image-sitemap-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-image-sitemap.ts"
      }
    },
    "seo-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-seo.ts"
      }
    },
    "tokens-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-tokens.ts"
      }
    },
    "token-drift-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-token-drift.ts"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node -e \"console.log('No web tests configured')\""
      }
    },
    "restore-generated-typings": {
      "executor": "nx:run-commands",
      "options": {
        "command": "git checkout -- apps/web/next-env.d.ts"
      }
    },
    "validate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx format:check --all",
          "nx run web:lint",
          "nx run web:content-validate",
          "nx run web:cookie-compliance-validate",
          "nx run web:docs-integrity-validate",
          "nx run web:governance-validate",
          "nx run web:compliance-registry-validate",
          "nx run web:tracker-usage-validate",
          "nx run web:telemetry-governance-validate",
          "nx run web:i18n-validate",
          "nx run web:pgf-validate",
          "nx run web:intelligence-validate",
          "nx run web:briefing-contracts-validate",
          "nx run web:authority-signals-validate",
          "nx run web:experience-intelligence-validate",
          "nx run web:frameworks-validate",
          "nx run web:aec-validate",
          "nx run web:orchestration-validate",
          "nx run web:content-os-validate",
          "nx run web:home-validate",
          "nx run web:sitemap-validate",
          "nx run web:image-sitemap-validate",
          "nx run web:media-manifest-validate",
          "nx run web:media-derivatives-validate",
          "nx run web:media-authority-validate",
          "nx run web:media-governance-validate",
          "nx run web:visual-contract-validate",
          "nx run web:seo-validate",
          "nx run web:tokens-validate",
          "nx run web:token-drift-validate",
          "nx run web:authority-program-validate",
          "nx run web:authority-constitution-validate",
          "nx run web:agent-overlay-validate",
          "nx run web:telemetry-scoring-validate",
          "nx run docs:arch:check",
          "nx run web:test"
        ],
        "parallel": false
      }
    },
    "verify": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "node -e \"try{require('fs').rmSync(require('path').join(process.cwd(),'apps/web/.next'),{recursive:true,force:true});}catch(e){}\"",
          "nx format:check --all",
          "pnpm audit --audit-level=critical",
          "nx run web:lint",
          "nx run web:security-validate",
          "nx run web:cookie-compliance-validate",
          "nx run web:content-validate",
          "nx run web:docs-integrity-validate",
          "nx run web:governance-validate",
          "nx run web:compliance-registry-validate",
          "nx run web:tracker-usage-validate",
          "nx run web:telemetry-governance-validate",
          "nx run web:i18n-validate",
          "nx run web:pgf-validate",
          "nx run web:intelligence-validate",
          "nx run web:briefing-contracts-validate",
          "nx run web:authority-signals-validate",
          "nx run web:experience-intelligence-validate",
          "nx run web:frameworks-validate",
          "nx run web:aec-validate",
          "nx run web:orchestration-validate",
          "nx run web:content-os-validate",
          "nx run web:home-validate",
          "nx run web:sitemap-validate",
          "nx run web:image-sitemap-validate",
          "nx run web:media-manifest-validate",
          "nx run web:media-derivatives-validate",
          "nx run web:media-authority-validate",
          "nx run web:media-governance-validate",
          "nx run web:visual-contract-validate",
          "nx run web:seo-validate",
          "nx run web:tokens-validate",
          "nx run web:token-drift-validate",
          "nx run web:authority-program-validate",
          "nx run web:authority-constitution-validate",
          "nx run web:agent-overlay-validate",
          "nx run web:telemetry-scoring-validate",
          "nx run docs:arch:check",
          "nx run web:test",
          "nx run web:build",
          "nx run web:restore-generated-typings",
          "npx cross-env SKIP_A11Y_BUILD=1 nx run web:a11y"
        ],
        "parallel": false
      }
    },
    "media-manifest-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx --tsconfig tsconfig.base.json tools/validate-media-manifest.ts"
      }
    },
    "media-derivatives-validate": {
